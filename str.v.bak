module str(f,clk,stop,res);//
output [7:0] f;
input clk,stop,res;
reg [7:0] f ;
integer k = 1;

always @ (posedge clk)
begin



if (k == 50000000)
begin
f<=8'b00000001;
end
if(k == 100000000)
begin
f<=8'b00000011;
end
if(k == 150000000)
begin
f<=8'b00000111;
end
if(k == 200000000)
begin
f<=8'b00001111;
end
if (k == 250000000)
begin
f<=8'b00011111;
end
if(k == 300000000)
begin
f<=8'b00111111;
end
if(k == 350000000)
begin
f<=8'b01111111;
end
if(k == 400000000)
begin
f<=8'b11111111;
end

if(k == 400000000)
begin
k<=1;
end

end
endmodule